{
  "functions": {
    "api/proxy.js": {
      "maxDuration": 60,
      "responseLimit": false
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS"
        }
      ]
    }
  ]
}
